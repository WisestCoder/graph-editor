{
  "name": "@ols-scripts/graph-editor",
  "version": "0.0.1",
  "description": "节点编辑器",
  "author": "mochen.du",
  "license": "ISC",
  "scripts": {
    "start": "npm run dev",
    "dev": "ols dev",
    "docs:build": "ols build --docs",
    "build": "ols build",
    "deploy": "ols deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "ols test",
    "test:coverage": "ols test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "d3-selection": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "dagre": "^0.8.5",
    "lodash": "^4.17.21",
    "react-draggable": "^4.4.3"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@ols-scripts/cli": "^0.0.1",
    "@ols-scripts/component-theme-one": "^0.0.1",
    "@ols-scripts/eslint-config": "^0.0.1",
    "@types/d3": "^6.3.0",
    "@types/dagre": "^0.7.44",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.19",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "@types/resize-observer-browser": "^0.1.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "typescript": "^3.8.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
