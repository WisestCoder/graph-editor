{
  "extends": ["./node_modules/@ols-scripts/eslint-config", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "globals": {
    "document": true,
    "localStorage": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": 1,
    "@typescript-eslint/prefer-for-of": 1,
    "react/jsx-curly-newline": 0,
    "no-shadow": "off",
    "no-underscore-dangle": 0
  }
}
